<AnchorPane fx:id="generateAP" minHeight="0.0" minWidth="0.0" prefHeight="512.0" prefWidth="525.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Rectangle arcHeight="5.0" arcWidth="5.0" height="527.0" stroke="BLACK" strokeType="INSIDE" width="525.0" />
      <ImageView fitHeight="217.0" fitWidth="268.0" layoutX="129.0" layoutY="8.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@generate.gif" />
         </image>
      </ImageView>
      <Label layoutX="92.0" layoutY="191.0" text="Type the number of systems you want to generate" textFill="WHITE">
         <font>
            <Font name="Century Gothic" size="14.0" />
         </font>
      </Label>
      <TextField fx:id="numberElementsGenerate" layoutX="187.0" layoutY="250.0" style="-fx-background-color: transparent; -fx-border-color: white; -fx-text-fill: white;" />
      <Button fx:id="generateButton" layoutX="230.0" layoutY="337.0" mnemonicParsing="false" onAction="#generate" style="-fx-background-color: yellow; -fx-border-color: white;" text="Generate" />
   </children>
</AnchorPane>
